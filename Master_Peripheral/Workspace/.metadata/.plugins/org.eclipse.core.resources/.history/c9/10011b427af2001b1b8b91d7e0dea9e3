#include <string.h>
#include <stdint.h>

// Create constant string in FLASH(ROM)
const char* Flash_Str = "Hello, I'm String from FLASH mem!\n";

// Create constant value in FLASH(ROM)
const uint8_t value =200;

// Create array char in SRAM
char SRAM_str[50];

int main(void)
{
    /* Loop forever */
	for (int i=0; i<strlen(Flash_Str); i++) {
		*SRAM_str = *(Flash_Str+i);
	}
	for(;;);
}
